views:
  - type: table
    name: Waldläuferzauber Grad 1
    filters:
      and:
        - file.tags.contains("Klasse/Waldläufer")
        - file.tags.contains("Zauber/Grad/1")
    order:
      - file.name
      - Zeitaufwand
      - Reichweite
      - Komponenten
      - Wirkungsdauer
      - Angriff
      - Schaden
      - status
